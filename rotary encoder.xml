      <ellipse centre="_Middle+10x+18y" radiusx="3" radiusy="3"/>
      <line start="_Middle-12x+12y" end="_Middle+12x+12y"/>
      <line start="_Middle-2x+10y" end="_Middle+2x+10y"/>
      <text value="A" x="_Middle-10" y="_Middle-4" align="CentreCentre"/>
      <text value="C" x="_Middle" y="_Middle-10" align="CentreCentre"/>
      <text value="B" x="_Middle+10" y="_Middle-4" align="CentreCentre"/>
