      <line start="_Start+20y" end="_Middle-20x+20y"/>
      <line start="_Middle-20x+20y" end="_Middle-10x+10y"/>
      <ellipse centre="_Middle" radiusx="24" radiusy="24" />
      <line start="_Middle+20x-10y" end="_End-10y"/>
      <ellipse centre="_Middle+18x-10y" radiusx="3" radiusy="3"/>
      <line start="_Middle+20x+10y" end="_End+10y"/>
