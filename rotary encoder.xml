      <line start="_Middle+12x-12y" end="_Middle+12x+12y"/>
      <line start="_Middle+10x-2y" end="_Middle+10x+2y"/>
      <text value="A" x="_Middle-5" y="_Middle-10" align="CentreCentre"/>
      <text value="C" x="_Middle-10" y="_Middle" align="CentreCentre"/>
      <text value="B" x="_Middle-5" y="_Middle+10" align="CentreCentre"/>
    </group>

    <group conditions="!_Horizontal">
      <line start="_Start-20x" end="_Middle-20x-20y"/>
      <line start="_Middle-20x-20y" end="_Middle-10x-10y"/>
      <line start="_Start" end="_Middle-20y"/>
      <line start="_Start+20x" end="_Middle+20x-20y"/>
