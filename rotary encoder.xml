</declaration> 

<connections>
  <group conditions="_Horizontal">
    <connection name="a" start="_Start-20x" end="_Middle-20x-20y" edge="start"/>
    <connection name="c" start="_Start" end="_Middle-20y" edge="start"/>
    <connection name="b" start="_Start+20x" end="_Middle+20x-20y" edge="start"/>
    <connection name="e" start="_Middle+20x+10y" end="_End" edge="end"/>
    <connection name="f" start="_Middle+20x+10y" end="_End" edge="end"/>
  </group>
  <group conditions="!_Horizontal">
    <connection name="a" start="_Start-20x" end="_Middle-20x-20y" edge="start"/>
    <connection name="c" start="_Start" end="_Middle-20x" edge="start"/>
    <connection name="b" start="_Start+20y" end="_Middle-20x+20y" edge="start"/>
    <connection name="e" start="_Middle-10x+20y" end="_End" edge="end"/>
    <connection name="f" start="_Middle+10x+20y" end="_End" edge="end"/>
  </group>
</connections>

<render>
    <group conditions="_Horizontal">
      <line start="_Start-20y" end="_Middle-20x-20y"/>
      <line start="_Middle-20x-20y" end="_Middle-10x-10y"/>
